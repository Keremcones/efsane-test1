{
  "functions": {
    "api/cors-proxy.js": {
      "regions": [
        "iad1"
      ]
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/home.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
